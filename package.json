{
  "name": "@nocturn-auth-server/web",
  "version": "1.0.0",
  "description": "Nocturne Auth Server web components",
  "author": "Leandro Silva Luz do Carmo",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "npm run build:base -- --watch",
    "build": "cross-env NODE_ENV=production npm run build:base",
    "build:base": "rollup -c rollup.config.js",
    "serve": "sirv build --cors --port 3002",
    "serve:dev": "sirv build --cors --port 3002 --dev",
    "vendor": "rollup -c rollup.config.vendor.js"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/cookieconsent": "^3.1.0",
    "autoprefixer": "^10.2.5",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "rollup": "^2.41.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.32.8",
    "sirv-cli": "^1.0.11"
  },
  "dependencies": {
    "bootstrap": "^4.6.0",
    "bootstrap-icons": "^1.4.0",
    "cookieconsent": "^3.1.1",
    "imask": "^6.1.0",
    "jquery": "^3.6.0",
    "jquery-validation": "^1.19.3",
    "jquery-validation-unobtrusive": "^3.2.12"
  }
}
